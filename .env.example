# Railway Environment Variables Configuration
# Copy this file to .env and fill in your values

# Database Configuration (Railway will provide this automatically)
DATABASE_URL=postgresql://username:password@host:port/database

# Session Security
SESSION_SECRET=your-super-secret-session-key-here

# Admin Account (optional - creates admin user on startup)
ADMIN_EMAIL=admin@your-domain.com
ADMIN_PASSWORD=secure-admin-password
ADMIN_FIRST_NAME=Admin
ADMIN_LAST_NAME=User

# OpenAI API (for circuit board analysis)
OPENAI_API_KEY=your-openai-api-key-here

# Environment
NODE_ENV=production

# Frontend URL (only needed if using separate domains)
# FRONTEND_URL=https://your-app.railway.app